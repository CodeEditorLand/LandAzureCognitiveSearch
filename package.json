{
	"name": "vscode-azuresearch",
	"displayName": "Azure Cognitive Search",
	"description": "Manage your Azure Cognitive Search services, create indexes, add documents, search, and more.",
	"version": "0.0.3",
	"publisher": "Microsoft",
	"aiKey": "AIF-d9b70cd4-b9f9-4d70-929b-a071c400b217",
	"license": "SEE LICENSE IN LICENSE.md",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Azure",
		"Snippets"
	],
	"keywords": [
		"Azure",
		"Cognitive",
		"Search",
		"Knowledge",
		"Mining"
	],
	"galleryBanner": {
		"color": "#0072c6",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/pablocastro/vscode-azuresearch"
	},
	"icon": "media/AzureSearch.png",
	"preview": true,
	"activationEvents": [
		"onView:azureSearch",
		"onCommand:azureSearch.refresh",
		"onCommand:azureSearch.openInPortal",
		"onCommand:azureSearch.selectSubscriptions",
		"onCommand:azureSearch.createDocument",
		"onCommand:azureSearch.createDataSource",
		"onCommand:azureSearch.createIndexer",
		"onCommand:azureSearch.createSkillset",
		"onCommand:azureSearch.createSynonymMap",
		"onCommand:azureSearch.deleteDocument",
		"onCommand:azureSearch.deleteDataSource",
		"onCommand:azureSearch.deleteIndexer",
		"onCommand:azureSearch.deleteSkillset",
		"onCommand:azureSearch.deleteSynonymMap",
		"onCommand:azureSearch.search",
		"onCommand:azureSearch.searchDoc"
	],
	"main": "main",
	"contributes": {
		"snippets": [
			{
				"language": "json",
				"path": "snippets/index-snippets.json"
			},
			{
				"language": "json",
				"path": "snippets/indexer-snippets.json"
			},
			{
				"language": "json",
				"path": "snippets/synonym-snippets.json"
			},
			{
				"language": "json",
				"path": "snippets/datasource-snippets.json"
			},
			{
				"language": "json",
				"path": "snippets/skillset-snippets.json"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "azure",
					"title": "Azure",
					"icon": "resources/azure.svg"
				}
			]
		},
		"views": {
			"azure": [
				{
					"id": "azureSearch",
					"name": "Search",
					"when": "config.azureSearch.showExplorer == true"
				}
			]
		},
		"commands": [
			{
				"command": "azureSearch.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				},
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.search",
				"title": "Search",
				"icon": {
					"light": "resources/light/search.svg",
					"dark": "resources/dark/search.svg"
				},
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.openInPortal",
				"title": "Open in Portal",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.selectSubscriptions",
				"title": "Select Subscriptions...",
				"icon": {
					"light": "resources/light/filter.svg",
					"dark": "resources/dark/filter.svg"
				},
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.createDocument",
				"title": "Create new document",
				"icon": {
					"light": "resources/light/add.svg",
					"dark": "resources/dark/add.svg"
				},
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.deleteDocument",
				"title": "Delete document",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.createDataSource",
				"title": "Create new data source",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.deleteDataSource",
				"title": "Delete data source",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.createIndexer",
				"title": "Create new indexer",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.openSearchEditor",
				"title": "Search index",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.deleteIndex",
				"title": "Delete index",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.createIndex",
				"title": "Create new index",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.deleteIndexer",
				"title": "Delete indexer",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.createSkillset",
				"title": "Create new skillset",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.deleteSkillset",
				"title": "Delete skillset",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.createSynonymMap",
				"title": "Create new synonym map",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.deleteSynonymMap",
				"title": "Delete synonym map",
				"category": "Azure Search"
			},
			{
				"command": "azureSearch.searchDoc",
				"title": "Search",
				"category": "Azure Search"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "azureSearch.refresh",
					"when": "view == azureSearch",
					"group": "navigation@99"
				},
				{
					"command": "azureSearch.search",
					"when": "view == azureSearch",
					"group": "navigation@97"
				},
				{
					"command": "azureSearch.createDocument",
					"when": "view == azureSearch && viewItem == azureSearchDocumentList",
					"group": "navigation@98"
				},
				{
					"command": "azureSearch.deleteDocument",
					"when": "view == azureSearch && viewItem == azureSearchDocument",
					"group": "navigation@98"
				}
			],
			"view/item/context": [
				{
					"command": "azureSearch.selectSubscriptions",
					"when": "view == azureSearch && viewItem == azureextensionui.azureSubscription",
					"group": "inline"
				},
				{
					"command": "azureSearch.refresh",
					"when": "view == azureSearch && viewItem == azureextensionui.azureSubscription",
					"group": "9_refresh"
				},
				{
					"command": "azureSearch.search",
					"when": "view == azureSearch && (viewItem == azureSearchDocumentList || viewItem == azureSearchIndex)",
					"group": "1_navigate"
				},
				{
					"command": "azureSearch.openInPortal",
					"when": "view == azureSearch && viewItem == azureSearchService",
					"group": "navigation@2"
				},
				{
					"command": "azureSearch.createDocument",
					"when": "view == azureSearch && viewItem == azureSearchDocumentList",
					"group": "2_edit@1"
				},
				{
					"command": "azureSearch.deleteDocument",
					"when": "view == azureSearch && viewItem == azureSearchDocument",
					"group": "2_edit@2"
				},
				{
					"command": "azureSearch.createDataSource",
					"when": "view == azureSearch && viewItem == azureSearchDataSourceList",
					"group": "navigation@98"
				},
				{
					"command": "azureSearch.deleteDataSource",
					"when": "view == azureSearch && viewItem == azureSearchDataSource",
					"group": "navigation@98"
				},
				{
					"command": "azureSearch.createIndexer",
					"when": "view == azureSearch && viewItem == azureSearchIndexerList",
					"group": "navigation@98"
				},
				{
					"command": "azureSearch.deleteIndexer",
					"when": "view == azureSearch && viewItem == azureSearchIndexer",
					"group": "navigation@98"
				},
				{
					"command": "azureSearch.createIndex",
					"when": "view == azureSearch && viewItem == azureSearchIndexList",
					"group": "navigation@98"
				},
				{
					"command": "azureSearch.openSearchEditor",
					"when": "view == azureSearch && viewItem == azureSearchIndex",
					"group": "navigation@98"
				},
				{
					"command": "azureSearch.deleteIndex",
					"when": "view == azureSearch && viewItem == azureSearchIndex",
					"group": "navigation@98"
				},
				{
					"command": "azureSearch.createSkillset",
					"when": "view == azureSearch && viewItem == azureSearchSkillsetList",
					"group": "navigation@98"
				},
				{
					"command": "azureSearch.deleteSkillset",
					"when": "view == azureSearch && viewItem == azureSearchSkillset",
					"group": "navigation@98"
				},
				{
					"command": "azureSearch.createSynonymMap",
					"when": "view == azureSearch && viewItem == azureSearchSynonymMapList",
					"group": "navigation@98"
				},
				{
					"command": "azureSearch.deleteSynonymMap",
					"when": "view == azureSearch && viewItem == azureSearchSynonymMap",
					"group": "navigation@98"
				}
			],
			"commandPalette": [
				{
					"command": "azureSearch.createDocument",
					"when": "never"
				},
				{
					"command": "azureSearch.deleteDocument",
					"when": "never"
				}
			],
			"editor/context": [
				{
					"command": "azureSearch.searchDoc",
					"when": "editorLangId == azuresearch"
				}
			],
			"editor/title": [
				{
					"command": "azureSearch.searchDoc",
					"when": "editorLangId == azuresearch"
				}
			]
		},
		"configuration": {
			"title": "Azure Search Services",
			"properties": {
				"azureSearch.showExplorer": {
					"type": "boolean",
					"default": true,
					"description": "Show or hide the Azure Search Explorer"
				}
			}
		},
		"jsonValidation": [
			{
				"fileMatch": "*.azsindex",
				"url": "./schema/index.json"
			},
			{
				"fileMatch": "*.azssymmap",
				"url": "./schema/synonymmap.json"
			},
			{
				"fileMatch": "*.azsds",
				"url": "./schema/datasource.json"
			},
			{
				"fileMatch": "*.azsindexer",
				"url": "./schema/indexer.json"
			},
			{
				"fileMatch": "*.azsskset",
				"url": "./schema/skillset.json"
			}
		],
		"languages": [
			{
				"id": "azuresearch",
				"extensions": [
					".azuresearch"
				],
				"aliases": [
					"AzureSearch"
				]
			}
		],
		"keybindings": [
			{
				"command": "azureSearch.searchDoc",
				"key": "ctrl+alt+r",
				"mac": "cmd+alt+r",
				"when": "editorLangId == azuresearch"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run webpack-prod",
		"build": "tsc",
		"compile": "tsc -watch",
		"package": "vsce package",
		"lint": "tslint --project tsconfig.json -t verbose",
		"lint-fix": "tslint --project tsconfig.json -t verbose --fix",
		"pretest": "npm run webpack-prod && gulp preTest",
		"test": "node ./out/test/runTest.js",
		"webpack": "npm run build && gulp webpack-dev",
		"webpack-prod": "npm run build && gulp webpack-prod",
		"webpack-profile": "webpack --profile --json --mode production > webpack-stats.json && echo Use http://webpack.github.io/analyse to analyze the stats",
		"all": "npm i && npm run lint && npm test"
	},
	"extensionDependencies": [
		"ms-vscode.azure-account"
	],
	"devDependencies": {
		"@types/fs-extra": "^8.1.1",
		"@types/glob": "^7.1.1",
		"@types/gulp": "^4.0.6",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@types/webpack": "^4.32.1",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"copy-webpack-plugin": "^5.0.3",
		"eslint": "^6.8.0",
		"file-loader": "^4.1.0",
		"glob": "^7.1.6",
		"gulp": "^4.0.2",
		"gulp-cli": "^2.2.0",
		"mocha": "^7.1.2",
		"ts-loader": "^6.0.4",
		"ts-node": "^8.3.0",
		"typescript": "^3.8.3",
		"vscode-azureextensiondev": "0.4.0",
		"vscode-test": "^1.3.0",
		"webpack": "^4.37.0",
		"webpack-cli": "^3.3.6"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"azure-arm-resource": "^7.3.0",
		"azure-arm-search": "^1.3.0-preview",
		"fs-extra": "^8.1.0",
		"vsce": "^1.77.0",
		"vscode-azureextensionui": "^0.33.1",
		"vscode-extension-telemetry": "^0.1.6"
	}
}