{
	"Skillset": {
		"prefix": "azs-skillset",
		"body": [
			"{",
			"\t\"description\": \"Extract text from images and merge with content text to produce merged_text\",",
			"\t\"skills\":",
			"\t[",
			"\t  {",
			"\t\t  \"description\": \"Extract text (plain and structured) from image.\",",
			"\t\t  \"@odata.type\": \"#Microsoft.Skills.Vision.OcrSkill\",",
			"\t\t  \"context\": \"/document/normalized_images/*\",",
			"\t\t  \"defaultLanguageCode\": \"en\",",
			"\t\t  \"detectOrientation\": true,",
			"\t\t  \"inputs\": [",
			"\t\t\t{",
			"\t\t\t  \"name\": \"image\",",
			"\t\t\t  \"source\": \"/document/normalized_images/*\"",
			"\t\t\t}",
			"\t\t  ],",
			"\t\t  \"outputs\": [",
			"\t\t\t{",
			"\t\t\t  \"name\": \"text\"",
			"\t\t\t}",
			"\t\t  ]",
			"\t  },",
			"\t  {",
			"\t\t\"@odata.type\": \"#Microsoft.Skills.Text.MergeSkill\",",
			"\t\t\"description\": \"Create merged_text, which includes all the textual representation of each image inserted at the right location in the content field.\",",
			"\t\t\"context\": \"/document\",",
			"\t\t\"insertPreTag\": \" \",",
			"\t\t\"insertPostTag\": \" \",",
			"\t\t\"inputs\": [",
			"\t\t  {",
			"\t\t\t\"name\":\"text\", \"source\": \"/document/content\"",
			"\t\t  },",
			"\t\t  {",
			"\t\t\t\"name\": \"itemsToInsert\", \"source\": \"/document/normalized_images/*/text\"",
			"\t\t  },",
			"\t\t  {",
			"\t\t\t\"name\":\"offsets\", \"source\": \"/document/normalized_images/*/contentOffset\" ",
			"\t\t  }",
			"\t\t],",
			"\t\t\"outputs\": [",
			"\t\t  {",
			"\t\t\t\"name\": \"mergedText\", \"targetName\" : \"merged_text\"",
			"\t\t  }",
			"\t\t]",
			"\t  }",
			"\t]",
			"  }"
		],
		"description": "Creates a skillset"
	},
