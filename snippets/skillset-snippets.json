	"Conditional Skill": {
		"prefix": "azs-skill-conditional",
		"body": [
			"{",
			"\t\"@odata.type\": \"#Microsoft.Skills.Util.ConditionalSkill\",",
			"\t\"context\": \"/document\",",
			"\t\"inputs\": [",
			"\t\t{ \"name\": \"condition\", \"source\": \"= $(/document/language) == 'fr'\" },",
			"\t\t{ \"name\": \"whenTrue\", \"source\": \"/document/sentences\" },",
			"\t\t{ \"name\": \"whenFalse\", \"source\": \"= null\" }",
			"\t],",
			"\t\"outputs\": [ { \"name\": \"output\", \"targetName\": \"frenchSentences\" } ]",
			"}"
		],
		"description": "Creates a conditional skill"
	},
